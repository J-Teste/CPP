NAME := harlFilter
CXX := g++
FLAGS := -Wall -Wextra -Werror -std=c++98 -pedantic

SRCDIR := ./src
OBJDIR := tmp

SRC := \
	main.cpp \
	Harl.cpp

SRCS := $(addprefix $(SRCDIR)/, $(SRC))

OBJ := $(addprefix $(OBJDIR)/, $(SRC:.cpp=.o))

all: $(NAME)

$(NAME): $(OBJ)
	@$(CXX) $(FLAGS) -o $@ $^
